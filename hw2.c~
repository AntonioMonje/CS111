// this program is for homework two that displays variables
#include <iostream>
using namespace std;

int main () 
{
  int day;
  int year;
  string courseName;
  int courseNumber;
  char letter;

  day = 9;
  year = 2015;
  courseName = "cs 111";
  courseNumber = 111;
  letter = 'I';
  cout << "Today is september" << day << ", " << year << "." << endl;
  cout << "The name of this course is " << courseName << " and its number is " << courseNumber << "." <<endl;  
  cout << "The first letter of word ipad is " << letter << "." << endl;
  return 0;
}
